{
  "name": "react-falcor",
  "version": "0.9.0",
  "description": "React binding for Falcor",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublish": "run-s clean build",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx,md .",
    "size": "size-limit",
    "test": "run-p test:* lint",
    "test:jest": "jest",
    "test:size": "run-s build size",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.44",
    "hoist-non-react-statics": "^2.5.0",
    "invariant": "^2.2.4",
    "lodash": "^4.17.5",
    "nanoevents": "^1.0.2",
    "prop-types": "^15.6.1",
    "recompose": "^0.26.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-react": "^7.0.0-beta.44",
    "@babel/preset-stage-0": "^7.0.0-beta.44",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^23.0.0-alpha.4",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.13",
    "cross-env": "^5.1.4",
    "del-cli": "^1.1.0",
    "delay": "^2.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-concise": "^0.22.0",
    "eslint-config-concise-esnext": "^0.22.0",
    "eslint-config-concise-react": "^0.20.0",
    "falcor": "^2.0.0",
    "falcor-json-graph": "^2.2.1",
    "falcor-router": "^0.8.1",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "npm-run-all": "^4.1.2",
    "prettier-eslint-cli": "^4.7.1",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-dom": "^16.3.1",
    "react-test-renderer": "^16.3.1",
    "size-limit": "^0.17.0"
  },
  "browserslist": [
    "last 2 versions",
    "IE >= 9"
  ],
  "repository": "ratson/react-falcor",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "falcor",
    "react"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/raf/polyfill.js",
      "<rootDir>/test/helpers/enzyme-config.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "13 KB"
    }
  ]
}
